<configuration>
    <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{MM-dd HH:mm:ss} [%p] %C{1}: %m%n</pattern>
        </encoder>
    </appender>

    <!--<logger name="com.ontotext" level="DEBUG" />-->

    <root level="INFO">
        <appender-ref ref="stdout"/>
    </root>
</configuration>